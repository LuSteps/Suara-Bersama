<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="20dp">

    <FrameLayout
        android:id="@+id/frameStackkinerjaterbukt"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:background="@drawable/border_textview">
        <LinearLayout android:id="@+id/linearColumnkinerjaterbukt"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="wrap_content"
            android:orientation="vertical" style="@style/groupStylewhite_3">
            <TextView android:ellipsize="end"
                android:id="@+id/txtKinerjaTerbukt"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{capresPageRowModel.txtKinerjaTerbukt}"
                style="@style/txtLailaregular10" tools:text="@string/msg_kinerja_terbukt"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtWawanSudrajat"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{capresPageRowModel.txtWawanSudrajat}"
                style="@style/txtLailaregular5" tools:text="@string/lbl_wawan_sudrajat"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtDescriptionTwo"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="match_parent"
                android:text="@{capresPageRowModel.txtDescriptionTwo}"
                style="@style/txtLailaregular6" tools:text="@string/msg_lorem_ipsum_dol2"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearColumncarbonlocation"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_3">

            <LinearLayout
                android:id="@+id/linearRowkinerjaterbuktOne"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:gravity="start"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/linearColumnkinerjaterbuktOne"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.82"
                    android:gravity="start"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtKinerjaTerbuktOne"
                        style="@style/txtLailaregular10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{capresPageRowModel.txtKinerjaTerbuktOne}"
                        android:textSize="18sp"
                        tools:text="@string/msg_kinerja_terbukt" />

                    <TextView
                        android:id="@+id/txtWawanSudrajatOne"
                        style="@style/txtLailaregular5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{capresPageRowModel.txtWawanSudrajatOne}"
                        android:textSize="10sp"
                        tools:text="@string/lbl_wawan_sudrajat" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/imageCarbonlocation"
                    android:layout_width="@dimen/_6pxh"
                    android:layout_height="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_carbonlocation"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_carbonlocation" />

                <TextView
                    android:id="@+id/txtSidoarjo"
                    android:textColor="@color/black_900_3f"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:textSize="10sp"
                    android:singleLine="true"
                    android:text="@string/lbl_sidoarjo" />
            </LinearLayout>

            <TextView
                android:gravity="fill"
                android:id="@+id/txtDescriptionOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_6pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:textSize="10sp"
                android:text="@{capresPageRowModel.txtDescriptionOne}"
                style="@style/txtLailaregular8" tools:text="@string/msg_lorem_ipsum_dol2"/>
        </LinearLayout>
    </FrameLayout>
</RelativeLayout>
