<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="wrap_content"
    android:layout_width="match_parent">

    <LinearLayout
        android:gravity="start"
        android:id="@+id/linearProfile"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">

        <LinearLayout
            android:id="@+id/linearColumngroupfifteen"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.0"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <TextView
                android:id="@+id/TitleHome"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/rectangle_bg_red_a400"
                android:ellipsize="end"
                android:fontFamily="@font/lalezar"
                android:gravity="start"
                android:lineSpacingExtra="-12dp"
                android:paddingStart="15dp"
                android:paddingTop="16dp"
                android:paddingBottom="13dp"
                android:singleLine="false"
                android:text="SUARA\nBERSAMA"
                android:textColor="#FFFFFFFF"
                android:textFontWeight="400"
                android:textSize="20sp"
                tools:ignore="RtlSymmetry,UnusedAttribute" />

            <TextView
                android:id="@+id/tentangKami"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="33dp"
                android:layout_marginTop="23dp"
                android:layout_marginEnd="33dp"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="Profile User"
                android:textSize="20sp"
                android:textColor="#FF000000"
                android:fontFamily="@font/lalezar"
                android:textStyle="normal"
                android:background="#FFFFFFFF"
                android:padding="8dp" />

            <LinearLayout
                android:id="@+id/linearColumnuser"
                style="@style/groupStylewhite_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_63pxv"
                android:layout_marginEnd="@dimen/_28pxh"
                android:gravity="center_horizontal"
                android:orientation="vertical">


                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="20dp"
                    android:text="Nama" />

                <EditText
                    android:id="@+id/editTextText4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="20dp"
                    android:ems="10"
                    android:inputType="text"
                    android:text=""
                    android:background="@drawable/border_textview"/>

                <TextView
                    android:id="@+id/Email"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="20dp"
                    android:text="Email" />

                <EditText
                    android:id="@+id/editTextText5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="20dp"
                    android:ems="10"
                    android:inputType="text"
                    android:text=""
                    android:background="@drawable/border_textview"/>

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="20dp"
                    android:text="Profesi" />

                <EditText
                    android:id="@+id/editTextText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="20dp"
                    android:ems="10"
                    android:inputType="text"
                    android:text=""
                    android:background="@drawable/border_textview"/>

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="20dp"
                    android:text="Jenis Kelamin" />


                <RadioGroup
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="20dp"
                    android:orientation="vertical">
                    <RadioButton android:id="@+id/radio_pirates"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Pria"/>
                    <RadioButton android:id="@+id/radio_ninjas"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Wanita"/>
                </RadioGroup>

                <Button
                    android:id="@+id/button"
                    android:layout_marginTop="15dp"
                    android:layout_width="100dp"
                    android:layout_height="60dp"
                    android:backgroundTint="@color/red_A400"
                    android:text="Simpan" />

            </LinearLayout>
        </LinearLayout>

        <FrameLayout
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            style="@style/groupStylered_A400">

            <LinearLayout android:id="@+id/linearRowhome"
                android:layout_height="match_parent"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:layout_width="match_parent" android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageHome"
                    android:layout_width="22dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="12dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_home" />
                <ImageView android:id="@+id/imageAntdesigncomm"
                    android:layout_gravity="center"
                    android:layout_height="27dp"
                    android:layout_marginBottom="11dp"
                    android:layout_marginStart="52dp"
                    android:layout_marginTop="11dp"
                    android:layout_width="30dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_antdesigncomm"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_antdesigncomm"/>
                <FrameLayout android:id="@+id/frameHealthiconscit"
                    android:layout_height="45dp"
                    android:layout_marginStart="40dp"
                    android:layout_gravity="center"
                    android:layout_width="58dp" style="@style/groupStylecornerRadius_1">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_gravity="center"
                        android:layout_height="42dp"
                        android:layout_width="42dp"
                        android:src="@drawable/img_user_white_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user_white_a700"/>
                </FrameLayout>
                <ImageView android:id="@+id/imageSearch"
                    android:layout_gravity="center"
                    android:layout_height="25dp"
                    android:layout_marginBottom="12dp"
                    android:layout_marginStart="46dp"
                    android:layout_marginTop="10dp"
                    android:layout_width="25dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                <ImageView android:id="@+id/imageUserOne"
                    android:layout_gravity="center"
                    android:layout_height="34dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginEnd="20dp"
                    android:layout_marginStart="50dp"
                    android:layout_marginTop="8dp"
                    android:layout_width="35dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</RelativeLayout>
