<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    android:id="@+id/frameStackpaslonno01"
    android:layout_height="wrap_content"
    android:layout_marginTop="40dp"
    android:layout_width="match_parent">


    <LinearLayout
        android:id="@+id/linearColumnpaslonnoOne"
        android:layout_gravity="start"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:background="@drawable/border_textview">
        <TextView
            android:id="@+id/txtPaslonNo01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="top|center"
            android:layout_marginStart="105dp"
            android:layout_marginEnd="105dp"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="9dp"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="Paslon No 1:"
            android:textSize="30sp"
            android:textColor="@color/black_900"
            android:fontFamily="@font/lalezar"/>

        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowgroup"
            android:layout_height="wrap_content"
            android:layout_marginBottom="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginStart="35dp"
            android:layout_marginTop="20dp"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView
                android:id="@+id/imageGroup01"
                android:layout_height="140dp"
                android:layout_width="120dp"
                android:layout_marginStart="40dp"
                android:scaleType="fitXY"
                android:src="@drawable/img_group"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group"/>

            <ImageView
                android:id="@+id/imageGroupOne1"
                android:layout_width="120dp"
                android:layout_height="140dp"
                android:layout_marginStart="20dp"
                android:scaleType="fitXY"
                android:src="@drawable/img_group"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_group" />
        </LinearLayout>

        <TextView
            android:id="@+id/namaPaslon"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:textSize="20sp"
            android:textColor="@color/black"
            android:text="Anies Baswedan - Muhaimin Iskandar"
            android:layout_marginBottom="20dp"/>
    </LinearLayout>
</FrameLayout>