<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:gravity="center_horizontal"
        android:id="@+id/linearLoginRegister"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylered_A400">
        <TextView android:ellipsize="end"
            android:id="@+id/txtSUARABERSAMA"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="35dp"
            android:layout_marginStart="35dp"
            android:layout_marginTop="100dp"
            android:layout_width="wrap_content"
            android:text="SUARA\nBERSAMA"
            style="@style/txtLalezarregular40"/>
        <EditText
            android:background="@drawable/rectangle_bg_white_a700_radius_4"
            android:ellipsize="end"
            android:hint="E-mail / No Telepon"
            android:id="@+id/etGroupTwentyNine"
            android:inputType="textEmailAddress"
            android:layout_height="wrap_content"
            android:layout_marginEnd="35dp"
            android:layout_marginStart="35dp"
            android:layout_marginTop="55dp"
            android:layout_width="match_parent"
            android:paddingBottom="8dp"
            android:paddingEnd="35dp"
            android:paddingStart="10dp"
            android:paddingTop="8dp"
            android:singleLine="true"
            android:text=""
            android:textColorHint="@color/gray_600"
            style="@style/etSolidRounded"
            tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_bg_white_a700_radius_4"
            android:ellipsize="end"
            android:hint="Password"
            android:id="@+id/etGroupTwentyEight"
            android:inputType="textPassword"
            android:layout_height="wrap_content"
            android:layout_marginEnd="35dp"
            android:layout_marginStart="35dp"
            android:layout_marginTop="30dp"
            android:layout_width="match_parent"
            android:paddingBottom="8dp"
            android:paddingEnd="35dp"
            android:paddingStart="10dp"
            android:paddingTop="8dp"
            android:singleLine="true"
            android:text=""
            android:textColorHint="@color/gray_600"
            style="@style/etSolidRounded"
            tools:ignore="Autofill"/>

        <Button
            android:id="@+id/button5"
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:backgroundTint="@color/orange_300"
            android:radius="8dp"
            android:text="Log-in" />

        <LinearLayout
            android:id="@+id/linearRowflatcoloricon"
            style="@style/groupStylewhite_1"
            android:layout_width="match_parent"
            android:layout_height="59dp"
            android:layout_marginStart="35dp"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="35dp"
            android:layout_marginBottom="130dp"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageFlatcoloricon"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_marginBottom="@dimen/_12pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_flatcoloricon"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_flatcoloricon" />

            <TextView
                android:id="@+id/txtSigninwithGo"
                style="@style/txtIntersemibold14_1"
                android:layout_width="194dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_75pxh"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_weight="0.84"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="Sign-in WIth Google" />
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>
